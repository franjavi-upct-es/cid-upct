\item[b)]  Producto: $z_1=(x_1,y_1),~z_2=(x_2,y_2)\in\mathbb{C}$
\textcolor{lightblue}{$z_1\cdot z_2=(x_1\cdot x_2-y_1\cdot
y_2,~~ x_1\cdot y_2+y_1\cdot x_2)\in\mathbb{C}$}
\end{enumerate}

