   "outputs": [],
   "source": [
    "tags_users= db.posts.aggregate( [\n",
    "        { \"$match\" : {\"PostTypeId\": 1}},\n",
    "\n",
    "        {\"$limit\": 50},\n",
    "\n",
    "        { '$lookup': {\n",
    "            'from': \"users\",\n",
    "            'localField': \"OwnerUserId\",\n",
    "            'foreignField': \"Id\",\n",
    "            'as': \"owner\"\n",
    "          }\n",
    "        },\n",
    "\n",
    "        {'$project':{\n",
    "            'Tags': True,\n",
    "            'userid' : {'$arrayElemAt' : ['$owner.Id', 0]},\n",
